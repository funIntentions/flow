    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Actuator -->

    <Class rdf:about="&smart-grid;Actuator">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&smart-grid;SensingAndControlDevice"/>
                    <Class>
                        <unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&smart-grid;InfraredGateway"/>
                            <rdf:Description rdf:about="&smart-grid;Inverter"/>
                            <rdf:Description rdf:about="&smart-grid;Relay"/>
                            <rdf:Description rdf:about="&smart-grid;SmartPlug"/>
                        </unionOf>
                    </Class>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;CommunicatesWith"/>
                <someValuesFrom rdf:resource="&smart-grid;AutomatedDemandResponceEndPoint"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Actuators are a group of control mechanisms.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Afternoon -->

    <Class rdf:about="&smart-grid;Afternoon">
        <rdfs:subClassOf rdf:resource="&smart-grid;TimeOfDay"/>
        <rdfs:comment>Afternoon represents the time between midday (12:00) and the start of the evening (18:00).</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Aggregator -->

    <Class rdf:about="&smart-grid;Aggregator">
        <rdfs:subClassOf rdf:resource="&smart-grid;Stakeholder"/>
        <rdfs:comment>Aggregator represents a single entity which manages a group of energy customers allowing the energy provider to treat said group as a single entity.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Apartment -->

    <Class rdf:about="&smart-grid;Apartment">
        <rdfs:subClassOf rdf:resource="&smart-grid;Site"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsLocatedIn"/>
                <someValuesFrom rdf:resource="&smart-grid;ResidentialOrCommercialBuilding"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsEquippedWith"/>
                <onClass rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsEquippedWith"/>
                <onClass rdf:resource="&smart-grid;InBuildingEnergyGenerationInfrastructure"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</qualifiedCardinality>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Appliance -->

    <Class rdf:about="&smart-grid;Appliance">
        <rdfs:subClassOf rdf:resource="&smart-grid;ElectricalDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasPower"/>
                <someValuesFrom rdf:resource="&xsd;double"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasStandbyConsumption"/>
                <someValuesFrom rdf:resource="&xsd;double"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasAverageConsumption"/>
                <someValuesFrom rdf:resource="&xsd;double"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>General household appliances that consume electricity (i.e. Washing Machines, TVs and Dish Washers).</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#AutomatedDemandResponceEndPoint -->

    <Class rdf:about="&smart-grid;AutomatedDemandResponceEndPoint">
        <rdfs:subClassOf rdf:resource="&smart-grid;CommunicationDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;CommunicatesWith"/>
                <someValuesFrom rdf:resource="&smart-grid;Concentrator"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyGenerationInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Automated Demand Responce End Point (ADR EP) works both as a network coodinator and as the communication gateway of a In-Building Consumption/Generation Infrastructure. Routes Information coming and going from the Power Saving Business Intelligence via the Concentrator.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Autumn -->

    <Class rdf:about="&smart-grid;Autumn">
        <rdfs:subClassOf rdf:resource="&smart-grid;Season"/>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#BackOfficeServer -->

    <Class rdf:about="&smart-grid;BackOfficeServer">
        <rdfs:subClassOf rdf:resource="&smart-grid;CommunicationDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;PowerSavingIntelligence"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Back-Office Server (BO Server) represents software responcible for the communications within the Power Saving Business Intelligence module. The BO Server sends and recieves data through the Gateway.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Biomass -->

    <Class rdf:about="&smart-grid;Biomass">
        <rdfs:subClassOf rdf:resource="&smart-grid;Resource"/>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#BiomassPowerPlant -->

    <Class rdf:about="&smart-grid;BiomassPowerPlant">
        <rdfs:subClassOf rdf:resource="&smart-grid;PowerPlant"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;TakesInput"/>
                <someValuesFrom rdf:resource="&smart-grid;Biomass"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;TakesInput"/>
                <allValuesFrom rdf:resource="&smart-grid;Biomass"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Building -->

    <Class rdf:about="&smart-grid;Building">
        <rdfs:subClassOf rdf:resource="&smart-grid;Domain"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsComposedOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyGenerationInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsComposedOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Represents the infrastructures associated with smart buildings and encompasses energy comsuption devices, generation sources and sensor and actuator networks.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Building_Prosumer -->

    <Class rdf:about="&smart-grid;Building_Prosumer">
        <rdfs:subClassOf rdf:resource="&smart-grid;Stakeholder"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;Owns"/>
                <someValuesFrom>
                    <Class>
                        <unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&smart-grid;Campus"/>
                            <rdf:Description rdf:about="&smart-grid;ResidentialOrCommercialBuilding"/>
                        </unionOf>
                    </Class>
                </someValuesFrom>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Campus -->

    <Class rdf:about="&smart-grid;Campus">
        <rdfs:subClassOf rdf:resource="&smart-grid;Site"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsEquippedWith"/>
                <onClass rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsEquippedWith"/>
                <onClass rdf:resource="&smart-grid;InBuildingEnergyGenerationInfrastructure"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#CarbonDioxide -->

    <Class rdf:about="&smart-grid;CarbonDioxide">
        <rdfs:subClassOf rdf:resource="&smart-grid;Emission"/>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Coal -->

    <Class rdf:about="&smart-grid;Coal">
        <rdfs:subClassOf rdf:resource="&smart-grid;Resource"/>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#ComfortSensor -->

    <Class rdf:about="&smart-grid;ComfortSensor">
        <rdfs:subClassOf rdf:resource="&smart-grid;SensingAndControlDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyConsumptionInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Comfort Sensors measure the different environmental variables, such as temperature, humidity or CO2 concentration. These factors are taken into consideration so that energy savings can be had without sacrificing comfort.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#CommunicationDevice -->

    <Class rdf:about="&smart-grid;CommunicationDevice">
        <rdfs:subClassOf rdf:resource="&smart-grid;Device"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasID"/>
                <someValuesFrom rdf:resource="&xsd;int"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasIP"/>
                <someValuesFrom rdf:resource="&xsd;int"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Communication Device represents devices that enable bidirectional communication between different Domains.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Concentrator -->

    <Class rdf:about="&smart-grid;Concentrator">
        <rdfs:subClassOf rdf:resource="&smart-grid;CommunicationDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;CommunicatesWith"/>
                <someValuesFrom rdf:resource="&smart-grid;Gateway"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;ConcentratorNetwork"/>
            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#ConcentratorNetwork -->

    <Class rdf:about="&smart-grid;ConcentratorNetwork">
        <rdfs:subClassOf rdf:resource="&smart-grid;Subsystem"/>
        <rdfs:comment>ConsentratorNetwork (CNTR) encompasses a group of devices that manage the access to the core of the Machine to Machine communications infrastructure.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Customer -->

    <Class rdf:about="&smart-grid;Customer">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&smart-grid;Stakeholder"/>
                    <Class>
                        <unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&smart-grid;Building_Prosumer"/>
                            <rdf:Description rdf:about="&smart-grid;Local_Energy_Producer"/>
                            <rdf:Description rdf:about="&smart-grid;Regional_Energy_Producer"/>
                            <rdf:Description rdf:about="&smart-grid;Residential_Consumer"/>
                            <rdf:Description rdf:about="&smart-grid;Residential_Prosumer"/>
                        </unionOf>
                    </Class>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:comment>Customer represents an individual that will subscribe to services offered by the Providers.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Device -->

    <Class rdf:about="&smart-grid;Device">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsLocatedIn"/>
                <someValuesFrom rdf:resource="&smart-grid;Site"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The Device represents the devices needed/involved in the development and implementation of the Smart Grid.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#DistributionGridManagement -->

    <Class rdf:about="&smart-grid;DistributionGridManagement">
        <rdfs:subClassOf rdf:resource="&smart-grid;Service"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;Involves"/>
                <someValuesFrom rdf:resource="&smart-grid;DistributionSystemOperator"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Distribution Grid Management (DGM) encompasses the group of services that provide the Distribution Service Operator with near real-time information about distributed energy generation and consumption. Data provided from the services specifies deviations from expected energy consumption behaviour and provides short-term forecasts for future energy generation. (helps DSO command Demand Responce programs)</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#DistributionSystemOperator -->

    <Class rdf:about="&smart-grid;DistributionSystemOperator">
        <rdfs:subClassOf rdf:resource="&smart-grid;Stakeholder"/>
        <rdfs:comment>Controls the medium to low voltage energy network, optimizing the operation of the power distribution grid. (preventing blackouts and ensuring reliability)</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Domain -->

    <Class rdf:about="&smart-grid;Domain">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsComposedOf"/>
                <someValuesFrom rdf:resource="&smart-grid;Subsystem"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Highest level of abstraction; this model is divided into the various domains that the ontology captures.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#ElectricalDevice -->

    <Class rdf:about="&smart-grid;ElectricalDevice">
        <rdfs:subClassOf rdf:resource="&smart-grid;Device"/>
        <rdfs:comment>Electrical Device represents the pure energy consumption, generation or storage devices.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Electricity -->

    <Class rdf:about="&smart-grid;Electricity">
        <rdfs:subClassOf rdf:resource="&smart-grid;Resource"/>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Emission -->

    <Class rdf:about="&smart-grid;Emission">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment>Emissions represent the greenhouse gas emissions commonly produced during the generation of electricity.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#EnergyStorageSystem -->

    <Class rdf:about="&smart-grid;EnergyStorageSystem">
        <rdfs:subClassOf rdf:resource="&smart-grid;ElectricalDevice"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;HasChargingRate"/>
                <someValuesFrom rdf:resource="&xsd;double"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&smart-grid;IsPartOf"/>
                <someValuesFrom rdf:resource="&smart-grid;InBuildingEnergyGenerationInfrastructure"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
