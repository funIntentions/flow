    <!-- http://www.semanticweb.org/dan/ontologies/2015/4/smart-grid#Changes -->

    <ObjectProperty rdf:about="&smart-grid;Changes">
        <rdfs:range rdf:resource="&smart-grid;Weather"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&smart-grid;Season"/>
                    <rdf:Description rdf:about="&smart-grid;TimeOfDay"/>
                </unionOf>
            </Class>
        </rdfs:domain>
    </ObjectProperty>
    


