<?xml version="1.0" encoding="UTF-8"?>

<smartGrid
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="SmartGridTemplate.xsd">
    <structures>
        <structure>
            <name>Home</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Apartment</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Apartment Building</name>
            <type>COMPOSITE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Power Plant</name>
            <type>POWER_PLANT</type>
            <properties>
                <property>
                    <name>HasCapitalCost</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasMonthlyExpenseOf</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasEfficiency</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasEmissionRate</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </structure>
    </structures>

    <devices>
        <device>
            <name>Appliance</name>
            <type>APPLIANCE</type>
            <properties>
                <property>
                    <name>HasStandbyConsumption</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasAverageConsumption</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasPower</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
        <device>
            <name>EnergySource</name>
            <type>ENERGY_SOURCE</type>
            <properties>
                <property>
                    <name>IsGenerating</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>HasRatedDCVoltage</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasCurrent</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasRatedACVoltage</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasPowerFactor</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
        <device>
            <name>EnergyStorage</name>
            <type>ENERGY_STORAGE</type>
            <properties>
                <property>
                    <name>IsCharging</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>IsReleasingEnergy</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>HasChargingRate</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasStoredEnergy</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>HasConnectionPower</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
    </devices>
</smartGrid>