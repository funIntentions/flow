<?xml version="1.0" encoding="UTF-8"?>

<smartGrid
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="SmartGridTemplate.xsd">
    <template>
        <structure>
            <name>Home</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Apartment</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Apartment Building</name>
            <type>COMPOSITE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Office Building</name>
            <type>COMPOSITE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Shop</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Mall</name>
            <type>COMPOSITE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Microgeneration Installation</name>
            <type>SINGLE_UNIT</type>
            <properties/>
        </structure>
        <structure>
            <name>Power Plant</name>
            <type>POWER_PLANT</type>
            <properties>
                <property>
                    <name>CapitalCost</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>MonthlyExpense</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>Efficiency</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>EmissionRate</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </structure>
    </template>

    <world>
    </world>

    <devices>
        <device>
            <name>Appliance</name>
            <type>APPLIANCE</type>
            <properties>
                <property>
                    <name>StandbyConsumption</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>AverageConsumption</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>Power</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
        <device>
            <name>EnergySource</name>
            <type>ENERGY_SOURCE</type>
            <properties>
                <property>
                    <name>IsGenerating</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>RatedDCVoltage</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>Current</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>RatedACVoltage</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>PowerFactor</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
        <device>
            <name>EnergyStorage</name>
            <type>ENERGY_STORAGE</type>
            <properties>
                <property>
                    <name>IsCharging</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>IsReleasingEnergy</name>
                    <value type="BOOLEAN">false</value>
                </property>
                <property>
                    <name>ChargingRate</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>StoredEnergy</name>
                    <value type="DOUBLE">0</value>
                </property>
                <property>
                    <name>ConnectionPower</name>
                    <value type="DOUBLE">0</value>
                </property>
            </properties>
        </device>
    </devices>
</smartGrid>